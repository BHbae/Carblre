<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
        namespace="com.carblre.repository.interfaces.LawyerRepository">


    <!-- 변호사 리스트 -->
    <select id="getAllLawyers" resultType="com.carblre.repository.model.LawyerDetail">
        SELECT
        user_id AS userId,
        uploard_profile_name AS uploadProfileName,
        introduction,
        law_firm AS lawFirm,
        office_num AS officeNum
        FROM
        lawyer_detail_tb
    </select>

</mapper>